#query to see the parsed logs
curl -XGET "http://localhost:9200/nginx*/_search" -H 'Content-Type: application/json' -d'{  "size": 1}'

#query to see if there are any un-parsed logs
curl -XGET "http://localhost:9200/nginx*/_search" -H 'Content-Type: application/json' -d'{
  "track_total_hits": true,
  "query": {
    "bool": {
      "must": [
        {
          "term": {
            "tags.keyword": "_grokparsefailure"
          }
        }
      ]
    }
  }
}'